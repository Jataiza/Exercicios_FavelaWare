programa {
	funcao inicio() {
	
		//Faça um programa que leia um número inteiro e mostre o antecessor e sucessor  desse número.
		
		inteiro antecessor,sucessor,numero
		
		escreva("Digite um numero:")
		leia(numero)
		
    	antecessor=numero-1
	    sucessor =numero+1
	    
	    escreva("\nO seu antecessor é:",antecessor )
	    escreva("\nO seu sucessor é:",sucessor)
	
	}
}
