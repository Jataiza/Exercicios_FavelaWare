programa {
	funcao inicio() {
		
		//Faça um programa que leia a temperatura em graus Celsius,(C) e apresentá-la convertida em graus Fahrenheit(F). Fórmula: F = (9*C+ 160)/5

    real  celsius, fahrenheit 

           escreva("Escreva o valor em graus Celsius:")
           leia(celsius)
           
           
           fahrenheit = (9*celsius+ 160)/5
           escreva("O valor em fahrenheit é:",fahrenheit)
		
	}
}
