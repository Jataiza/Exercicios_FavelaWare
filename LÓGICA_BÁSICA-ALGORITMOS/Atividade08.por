programa {
	funcao inicio() {
	
	//Faça um programa que leia as dimensões de um retângulo (basee altura), calcule e escreva a área do retângulo. Fórmula: base * altura
	 
	 real base,altura,retangulo
		
	    escreva("Digite o valor da base do seu retângulo :")
	    leia(base)
	  
	     escreva("Digite o valor da altura do seu retângulo :")
	     leia(altura)
	     
	     retangulo= base*altura
	     
	      escreva("As dimensões do um retângulo é:",retangulo)
	}
}
